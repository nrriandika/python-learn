artist[4] = "Dian Sastro"
print(artist[4])
artist = ["Pevita Pearce","Isyana Sarasvati","Marsha Aruan", "Yuki Kato", "Irish Bella" ]
number = [8,3,42,6,36,]
artist.extend(number)
